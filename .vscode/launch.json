{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Home",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000/home/index.html",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "skipFiles": [
                "<node_internals>/**",
                "node_modules/**",
                "**/.next/**",
                "**/next/**",
                "**/*turbopack*/**",
                "**/turbopack/**",
                "**/*hmr-client*",
                "**/*webpack*",
                "**/proxy.js",
                "**/*bundle*",
                "**/*runtime*",
                "**/*polyfills*"
            ],
            "userDataDir": "${workspaceFolder}/.vscode/chrome-debug-profile"
        },
        {
            "name": "Builder",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000/builder?page=business-starter",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "skipFiles": [
                "<node_internals>/**",
                "node_modules/**",
                "**/.next/**",
                "**/next/**",
                "**/*turbopack*/**",
                "**/turbopack/**",
                "**/*hmr-client*",
                "**/*webpack*",
                "**/proxy.js",
                "**/*bundle*",
                "**/*runtime*",
                "**/*polyfills*"
            ],
            "userDataDir": "${workspaceFolder}/.vscode/chrome-debug-profile"
        }
    ]
}
